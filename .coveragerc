[run]
source = retrofitkit
omit =
    */tests/*
    */test_*.py
    setup.py
    */venv/*
    */.venv/*
    */bentoml_service/*
    */scripts/*

[report]
precision = 2
show_missing = True
skip_covered = False

exclude_lines =
    pragma: no cover
    def __repr__
    raise AssertionError
    raise NotImplementedError
    if __name__ == .__main__.:
    if TYPE_CHECKING:
    class .*\bProtocol\):
    @(abc\.)?abstractmethod

[html]
directory = htmlcov

[xml]
output = coverage.xml
